{
    "browser": false,
    "node": true,
    "bitwise": false,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": false,
    "freeze": true,
    "funcscope": false,
    "globalstrict": false,
    "immed": true,
    "indent": 4,
    "iterator": false,
    "latedef": "nofunc",
    "maxcomplexity": false,
    "maxdepth": 10,
    "maxerr": 100,
    "maxlen": 80,
    "maxparams": 3,
    "maxstatements": 100,
    "newcap": false,
    "noarg": true,
    "nocomma": false,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "notypeof": false,
    "plusplus": false,
    "quotmark": "single",
    "shadow": true,
    "undef": true,
    "unused": true,
    "strict": false
}